<div class="jobseeker-create-container">

    <nav class="navbar navbar-background flex-container-center" role="navigation"> <!--fixed-nav-->
        <div class="navbar-header col-sm-4">
            <a class="navbar-brand logo-container" href="/">
                <!--<span><i class="fa fa-foursquare" aria-hidden="true"></i>&nbsp;</span>-->
                <span class="logo"><span><i class="fa fa-foursquare" aria-hidden="true"></i></span>acultyNaukri</span>
                <span class="logo-sub">.com</span>
            </a>
        </div>
        <div class="col-sm-4 collapse padding-top-5 navbar-collapse menu-items" uib-collapse="isNavCollapsed">
            <ul class="nav navbar-nav">
                <li><a href="/">Home</a></li>
                <li><a href="/#about-us">About Us</a></li>
                <li><a href="#footer">Contact Us</a></li>
            </ul>
        </div>
        <div class="col-sm-4 sign-in-links">
            <ul class="nav navbar-nav pull-right">
                <li>
                    <a  href="/login">
                        <div class="text-center">
                            <div class="small-text">Jobseeker</div>
                            <div>SIGN IN</div>
                        </div>
                    </a>
                </li>
                <li>
                    <a href="/recruiter/new">
                        <div class="text-center">
                            <div class="small-text">Recruiter</div>
                            <div>REGISTER HERE</div>
                        </div>
                    </a>
                </li>
            </ul>
        </div>
    </nav>
    <hr>


    <div class="shadow-box">
        <uib-tabset active="activeJustified" justified="true">
            <uib-tab class="active" index="0" heading="Personal 1"></uib-tab>
            <uib-tab class="disabled" index="1" heading="Personal 2" disable="true">Short Labeled Justified content</uib-tab>
            <uib-tab class="disabled" index="2" heading="Job Specifics" disable="true">Long Labeled Justified content</uib-tab>
        </uib-tabset><br><br>

        <form id="jobseeker-form" name="jobSeekerFrom">
            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_firstname">First Name</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <input class="form-control" placeholder="First Name" name="j_firstname" type="text" />
                    </div>
                </div>
            </div>


            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_lastname">Last Name</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <input class="form-control" placeholder="Last Name" name="j_lastname" type="text" />
                    </div>
                </div>
            </div>


            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_email">Email</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <input class="form-control" placeholder="Email Address" name="j_email" type="email" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_password">Password</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <input class="form-control" placeholder="Password" name="j_password" type="password" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_contact">Contact</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <input class="form-control" placeholder="Contact Number" name="j_contact" type="text" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_dob">DOB</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group">
                        <input name="j_dob" type="text" class="form-control" uib-datepicker-popup
                               ng-model="dt" is-open="popup2.opened"
                               datepicker-options="dateOptions" required="true" close-text="Close" />
                      <span class="input-group-btn calender-button">
                        <button type="button" class="btn btn-default" ng-click="open2()"><i class="glyphicon glyphicon-calendar"></i></button>
                      </span>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_gender">Gender</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group col-sm-10">
                        <div class="btn-group">
                            <label class="radio-inline">
                                <input type="radio" name="j_gender">Male
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="j_gender">Female
                            </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_interest">Interested In</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <!-- Single button -->
                        <select name="j_interest" class="form-control">
                            <option value="default">Choose the field you are interested to teach</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                        </select>
                        <!--<select name="j_state" class="form-control" ng-model="placement.selected" ng-options="o as o for o in placement.options"></select>-->
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="label-wrapper col-sm-2">
                    <label class="control-label" for="j_city">City</label>
                </div>
                <div class="input-group-wrapper col-sm-10">
                    <div class="input-group full-width">
                        <!-- Single button -->
                        <select name="j_city" class="form-control">
                            <option value="default">Choose...</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                        </select>
                        <!--<select name="j_city" class="form-control" ng-model="placement.selected" ng-options="o as o for o in placement.options"></select>-->
                    </div>
                </div>
            </div><br>
            <div class="submit-container">
                <button type="submit" class="btn btn-width-70 btn-custom-settings">Register Now</button><br><br>
            </div>
        </form>
    </div><br><br><br>

</div>
