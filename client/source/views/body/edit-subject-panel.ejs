    <form novalidate id="update-vacancy-form" name="updateVacancyForm">

        <div class="form-group">
            <div class="label-wrapper col-sm-3">
                <label class="control-label" for="sub_level">You are looking for</label>
            </div>
            <div class="input-group-wrapper col-sm-9">
                <div class="input-group">
                    <div class="btn-group">
                        <label class="radio-inline">
                            <input ng-model="editedSubject.subject_type" ng-change="levelChanged(1)" value="1"
                                   type="radio" name="sub_level" >Coaching Level
                        </label>
                        <label class="radio-inline">
                            <input ng-model="editedSubject.subject_type" ng-change="levelChanged(2)" value="2"
                                   type="radio" name="sub_level" >School Level
                        </label>
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="label-wrapper col-sm-3">
                <label class="control-label" for="sub_subject">Choose Subject</label>
            </div>
            <div class="input-group-wrapper col-sm-9">
                <div class="input-group full-width">
                    <!-- Single button -->
                    <select ng-model="editedSubject.subject_id" name="sub_subject" class="form-control">
                        <option disabled selected value> -- Choose the subject you are about to edit -- </option>
                        <option ng-repeat="subject in subjectsList" value="{{subject.subject_id}}" >
                            {{subject.subject_name}}
                        </option>
                    </select>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="label-wrapper col-sm-3">
                <label class="control-label" for="sub_vacancies">Vacancies</label>
            </div>
            <div class="input-group-wrapper col-sm-9">
                <div class="input-group full-width">
                    <input ng-model="editedSubject.subject_vacancies" class="form-control"
                           placeholder="Number of vacancies currently" name="sub_vacancies" type="number" />
                </div>
            </div>
        </div><br><br>

        <div class="submit-container flex-container-center">
            <button id="update-vacancies-submit" type="submit" class="btn btn-width-70 btn-custom-settings">Update Vacancies</button><br><br>
        </div><br>

        <div class="form-group flex-container-center">
            <p>Click <a href="/admin/add">here</a> to add a subject</p>
        </div><br>

        </form>

